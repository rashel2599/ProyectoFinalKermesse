@model ProyectoFinalKermesse.Models.TasaCambioDet

@{
    ViewBag.Title = "Detalles";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div>

    <ol class="breadcrumb" style="background:none; font-size: 18px">
        <li><a href="#">  @Html.ActionLink("Inicio", "Index", "Home", new { area = "" }, new { @style = "color: #779FA1" })  </a></li>
        <li><a href="#">  @Html.ActionLink("Gestión de tablas", "Index", "Gestion", new { area = "" }, new { @style = "color: #779FA1" })  </a></li>
        <li><a href="#">  @Html.ActionLink("Tabla Detalle de Tasa de Cambio", "Index", "TasaCambioDets", new { area = "" }, new { @style = "color: #779FA1" })  </a></li>
        <li class="active"> Detalles  </li>
    </ol>
</div>

<h2>Detalles</h2>

<div>
    <h4>Tabla Detalle de Tasa de Cambion</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @*@Html.DisplayNameFor(model => model.fecha)*@
            Fecha
        </dt>

        <dd>
            @Html.DisplayFor(model => model.fecha)
        </dd>

        <dt>
            @*@Html.DisplayNameFor(model => model.tipoCambio)*@
            Tipo de Cambio
        </dt>

        <dd>
            @Html.DisplayFor(model => model.tipoCambio)
        </dd>

        <dt>
            @*@Html.DisplayNameFor(model => model.estado)*@
            Estado
        </dt>

        <dd>
            @Html.DisplayFor(model => model.estado)
        </dd>

        <dt>
            @*@Html.DisplayNameFor(model => model.TasaCambio1.mes)*@
            Tasa de Cambio
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TasaCambio1.mes)
        </dd>

    </dl>
</div>
<p>
    <button class="btn-primary" style="background-color: #779FA1">
        <span class="glyphicon glyphicon-hand-left" aria-hidden="true" style="float: left; color: #FFFFFF"></span>
        @Html.ActionLink("Regresar", "Index", "TasaCambioDets", new { @style = "color:white;" })
    </button>

    <button type="submit" class="btn-primary" style="background-color: #779FA1">
        <span class="glyphicon glyphicon-edit" aria-hidden="true" style="float: left; color: #FFFFFF"></span>
        @Html.ActionLink("Editar", "Edit", new { id = Model.idTasaCambioDet }, new { @style = "color:white;" })
    </button>
</p>
